@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_liquiddrive_#_liquiddrive_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxliquiddrive.html> ;
        modgui:stylesheet <modgui/stylesheet-gxliquiddrive.css> ;
        modgui:screenshot <modgui/screenshot-gxliquiddrive.png> ;
        modgui:thumbnail <modgui/thumbnail-gxliquiddrive.png> ;
        modgui:brand "Guitarix " ;
        modgui:label "GxLiquidDrive" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "liquiddrive" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DRIVE" ;
            lv2:name "DRIVE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "LEVEL" ;
            lv2:name "LEVEL" ;
        ] ;
    ] .